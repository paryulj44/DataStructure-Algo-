void splitList(circular_LinkedList list)
        {
            Node slow = list.head, fast = list.head;
            Node ahead = null;
            while(fast.next != list.head && fast.next.next != list.head){
                slow = slow.next;
                fast = fast.next.next;
            }
            // ahead = slow.next;
            // slow.next = list.head;
            // list.head1 = list.head;
            // Node curr = ahead;
            // while(curr.next != list.head){
            //     curr = curr.next;
            // }
            // curr.next = ahead;
            // list.head2 = ahead;
            ahead = slow.next; 
            slow.next = list.head;
            list.head1 = list.head;
            list.head2 = ahead;

            
            while(ahead.next != list.head ){
                ahead = ahead.next;
            }
            ahead.next = list.head2;
	 }
